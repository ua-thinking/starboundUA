  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/story/baron.npctype": [
        "/scriptConfig/dialog/baron/beamOut/default/default/0"
      ]
    },
    "Texts": {
      "Eng": "Invigorated. Out into the great unknown!"
