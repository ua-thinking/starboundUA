  },
  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/story/baron.npctype": [
        "/scriptConfig/dialog/baron/finalWaveComplete/default/default/2"
      ]
    },
    "Texts": {
      "Eng": "Shocked. What's that in the sky?!"
