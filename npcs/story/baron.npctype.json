    "Texts": {
      "Eng": "Affable. Hello, stranger! Welcome to my keep!"
    }
  },
  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/story/baron.npctype": [
        "/scriptConfig/dialog/baron/start/default/default/2"
      ]
    },
    "Texts": {
      "Eng": "Attentive. What's that? You have a request? The thing behind me? Oh, that..."
